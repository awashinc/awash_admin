    <link href="../vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    -->

    <!-- Custom Theme Style -->
    <%#= stylesheet_link_tag "gentelella/custom.min" , media: 'all', 'data-turbolinks-track': 'reload' %>

