    <%#= stylesheet_link_tag "bootstrap-progressbar/bootstrap-progressbar-3.3.4.min" , media: 'all', 'data-turbolinks-track': 'reload' %>
    <!-- JQVMap -->
    <%#= stylesheet_link_tag "jqvmap/jqvmap.min" , media: 'all', 'data-turbolinks-track': 'reload' %>
