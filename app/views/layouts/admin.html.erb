    <!-- NProgress -->
    <!--
    <link href="../vendors/nprogress/nprogress.css" rel="stylesheet">
    -->
    <!-- iCheck -->
    <%#= stylesheet_link_tag "iCheck/skins/flat/green" , media: 'all', 'data-turbolinks-track': 'reload' %>
