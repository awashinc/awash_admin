<!DOCTYPE html>
<html>
  <head>
    <title>Awash</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'admin/application',"admin/#{controller_name}" , media: 'all', 'data-turbolinks-track': 'reload' %>
